    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.54.4",
    "terser": "^5.14.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.11.1",
    "unplugin-icons": "^0.14.8",
