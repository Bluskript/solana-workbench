  "devEngines": {
    "node": ">=16.15.0",
    "npm": ">=8.x"
  },
  "//": "this is in order to get electron-gyp to not fail with a build error",
  "overrides": {
    "nan": "github:jkleinsc/nan#remove_accessor_signature"
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
